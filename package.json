{
  "name": "when-will-i-be-famous",
  "version": "0.0.1",
  "description": "Trying out the famo.us engine",
  "browser": "src/index.js",
  "author": "Patrick Hund <wiekatz@gmail.com>",
  "license": "MIT",
  "scripts": {
    "install": "browserify src/index.js -g uglifyify | uglifyjs --screw-ie8 -m -c dead_code,sequences,conditionals,booleans,unused,if_return,join_vars,drop_debugger > public/bundle.js",
    "start": "watchify src/index.js -d -v -o public/bundle.js & serve public/ -p 1618",
    "test": "eslint --ignore-path .gitignore src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Famous/engine-seed.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "browserify": "^10.1.3",
    "eslint": "^0.22.1",
    "serve": "^1.4.0",
    "uglify-js": "^2.4.15",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  },
  "dependencies": {
    "babelify": "^6.0.1",
    "famous": "git://github.com/famous/engine"
  }
}
